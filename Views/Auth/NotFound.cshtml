@{
    Layout = ViewData["Layout"].ToString();
    var HomeURL = ViewData["HomeURL"].ToString();
}
<center class="pt-5 pb-5">
    <div class="mb-5">
        <img class="access-denied-img" src="~/img/404.png"/>
    </div>
    <div class="mt-5">
        <h3>Not Found</h3>
    </div>
    <div class="mt-3">
        <p>Sorry, we can't find the page you're looking for</p>
    </div>
    <div class="mt-3">
        <button class="btn btn-primary btn-block snw-submit" style="width: 200px;" onclick="goHome(@HomeURL)">Back to Home</button>
    </div>
</center>

@section Scripts{
    <script>
        function goHome(url){
            window.location.href = url;
        }
        $(document).ready(function () {
            $(".snw-img-header").addClass("d-none");
        });
    </script>
}